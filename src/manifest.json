{
    "name" : "Put New Tab Right",

    "description" : "Simple extension for move new tab to the right of the active tab.",

    "icons": {
        "16"     : "img/logo_16.png",
        "32"     : "img/logo_32.png",
        "48"     : "img/logo_48.png",
        "64"     : "img/logo_64.png",
        "128"    : "img/logo_128.png"
    },

    "version" : "0.0.1",

    "manifest_version" : 2,

    "permissions" : [
        "tabs"
    ],

    "background" : {
        "scripts" : ["background.js"]
    },

    "browser_action" : {
        "default_icon" : "img/logo_16.png"
    }
}

